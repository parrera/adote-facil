{
  "type": "module",
  "scripts": {
    "start": "/wait-for adote-facil-postgres:5432 -- npm run migrate:prod && node dist/server.js",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev --name",
    "migrate:rollback": "prisma migrate reset --force --skip-seed",
    "migrate:prod": "prisma migrate deploy",
    "dev": "tsx watch src/server.ts",
    "build": "swc src --out-dir dist --strip-leading-paths",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.2.2",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.23",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "prettier": "^3.6.0",
    "prisma": "^5.19.1",
    "tsx": "^4.19.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1"
  }
}
